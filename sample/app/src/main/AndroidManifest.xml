<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.marcoscg.fabscrollanimationsexample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DefaultFAB"
            android:label="Default FAB Animation"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".FadeFAB"
            android:label="Fade FAB Animation"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".FloatFAB"
            android:label="Float FAB Animation"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>
